{
  "name": "kickpress",
  "version": "1.0.1",
  "description": "A fast and opinionated CLI for scaffolding Express.js projects with TypeScript, Prisma, and best practices built-in",
  "main": "dist/index.js",
  "type": "module",
  "files": [
    "dist",
    "README.md"
  ],
